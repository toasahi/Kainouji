{
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    }
  }